{
  "name": "vue3-tournament",
  "version": "1.1.1",
  "author": "Balal Arast <arastbalal@gmail.com>",
  "description": "Tournament brackets generator using Vue 3.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balalarast/vue3-tournament.git"
  },
  "bugs": {
    "url": "https://github.com/balalarast/vue3-tournament/issues"
  },
  "homepage": "https://github.com/balalarast/vue3-tournament#readme",
  "keywords": [
    "vue",
    "vue3",
    "tournament",
    "bracket"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/tournament-bracket.umd.js",
  "module": "dist/tournament-bracket.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tournament-bracket.mjs",
      "require": "./dist/tournament-bracket.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./interface/*": {
      "types": "./dist/interface/*.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc ",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.10"
  },
  "devDependencies": {
    "@types/node": "^22.7.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6"
  }
}
